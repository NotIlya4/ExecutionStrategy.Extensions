<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Authors>NotIlya</Authors>
        <LangVersion>latest</LangVersion>
        <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
        <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <EmbedUntrackedSources>true</EmbedUntrackedSources>
        <IncludeSymbols>true</IncludeSymbols>
        <Copyright>MIT</Copyright>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <AssemblyName>EntityFrameworkCore.ExecutionStrategyExtended</AssemblyName>
        <RootNamespace>EntityFrameworkCore.ExecutionStrategyExtended</RootNamespace>
        <PackageProjectUrl>https://github.com/NotIlya4/ExecutionStrategyExtended</PackageProjectUrl>
        <RepositoryUrl>https://github.com/NotIlya4/ExecutionStrategyExtended</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>ef-core iexecutionstrategy</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6" />
      <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="6.0.0" />
        <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="ExecutionStrategyExtended.csproj.DotSettings" />
    </ItemGroup>

</Project>
